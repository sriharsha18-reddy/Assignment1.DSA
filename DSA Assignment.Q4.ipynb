   "source": [
    "#4.Write a program to print the first non-repeated character from a string?\n",
    "String = \"sriharshareddy\"\n",
    "for i in String:\n",
    "    count = 0\n",
    "    for j in String:\n",
    "        if i == j:\n",
    "            count+=1\n",
    "        if count > 1:\n",
    "            break\n",
    "    if count == 1:\n",
    "        print(i,end = \" \")"
   ]
  }
